<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" 
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:p="http://primefaces.org/ui">
<h:head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
	<link href="css/style1.css" rel="stylesheet" type="text/css" />
</h:head>


<h:body dir="#{facesBean.direction}">
	<f:view locale="#{facesBean.locale}">
		<ui:include src="#{facesBean.header}"></ui:include>
		<ui:include src="#{facesBean.menu}"></ui:include>
		<h:panelGroup layout="block" class="container">
			<h:panelGroup layout="block" class="main_div">

				<h:panelGrid columns="1" width="100%" styleClass="heading1">
					<h:outputText value="#{msg['lbl.corporateregistration']}"
						style="text-align:center !important; display:block;" />
				</h:panelGrid>
				<h:panelGroup layout="block" class="table_bg">
					<p:panel id="mainPanel" style="padding:10px!important;">

						<h:form id="corporateregistrationfrm" enctype="multipart/form-data">
							<p:panel id="mainPanel"
								rendered="#{corpregisterBean.coAMLCommon}"
								style="align: left; width:820px;border:none;">
							
								<p:panelGrid  cellpadding="0" cellspacing="15"
									style="text-align:left;width:100%">
									<p:row>
									<p:column>
									<h:outputText
										value="#{msg['lbl.crnumber']} #{corpregisterBean.viewBehaviorBean('Cr Number','MANDATORY')}" />
                                    </p:column>
                                    <p:column>
									<p:inputText id="cr" value="#{corpregisterBean.crno}"
										readonly="#{corpregisterBean.readOnly}"
										required="#{corpregisterBean.viewBehaviorBean('Cr Number','REQUIRED')}"
										maxlength="#{corpregisterBean.viewBehaviorBean('Cr Number','MAX_VALUE')}"
										minlength="#{corpregisterBean.viewBehaviorBean('Cr Number','MIN_VALUE')}"
										validatorMessage="#{msg['lbl.validcrno']}" text-align="left"
										requiredMessage="#{msg['lbl.validationprefix']} #{msg['lbl.crnumber']}">
										<p:ajax event="blur" listener="#{corpregisterBean.fetchData}"
											update="mainPanel,mainPanel1" />
										<p:message for="cr" display="text" />
									</p:inputText>
									</p:column>
									<p:column></p:column>
									<p:column></p:column>
                                    </p:row>
									<p:row>
                                     <p:column>
                                        <h:outputText
										value="#{msg['lbl.companyname']} #{corpregisterBean.viewBehaviorBean('Company Name','MANDATORY')}" />
                                     </p:column>
									<p:column>
									     <p:inputText id="name" value="#{corpregisterBean.compName}"
										readonly="#{corpregisterBean.readOnly}"
										required="#{corpregisterBean.viewBehaviorBean('Company Name','REQUIRED')}"
										validatorMessage="#{msg['lbl.validcompanyname']}"
										requiredMessage="#{msg['lbl.validationprefix']} #{msg['lbl.companyname']}"
										maxlength="#{corpregisterBean.viewBehaviorBean('Company Name','MAX_VALUE')}"
										minlength="#{corpregisterBean.viewBehaviorBean('Company Name','MIN_VALUE')}">
										<f:validateRegex
											pattern="#{corpregisterBean.viewBehaviorBean('Company Name','PATTERN')}" />
										<p:message for="name" display="text" />
								    	</p:inputText>
									</p:column>
									<p:column>
									    <h:outputText
										value="#{msg['lbl.companynamel']} #{corpregisterBean.viewBehaviorBean('Company Name(L)','MANDATORY')}" />
									
									</p:column>
									<p:column>
									   <p:inputText id="cnamel" value="#{corpregisterBean.compNameL}"
										readonly="#{corpregisterBean.readOnly}"
										required="#{corpregisterBean.viewBehaviorBean('Company Name(L)','REQUIRED')}"
										requiredMessage="#{msg['lbl.validationprefix']} #{msg['lbl.companynamel']}"
										maxlength="#{corpregisterBean.viewBehaviorBean('Company Name(L)','MAX_VALUE')}"
										minlength="#{corpregisterBean.viewBehaviorBean('Company Name(L)','MIN_VALUE')}">
										<p:message for="cnamel" display="text" />
									</p:inputText>
									</p:column>
									</p:row>
									<p:row>
									 <p:column>
									     <h:outputText
										value="#{msg['lbl.companyaregdate']} #{corpregisterBean.viewBehaviorBean('Company Registration Date','MANDATORY')}" />
									 </p:column>
									 <p:column>
									     <p:calendar id="regdate"
										value="#{corpregisterBean.compRegDate}" autocomplete="off"
										readonlyInput="true" pattern="dd/MM/yyyy" navigator="true"
										required="#{corpregisterBean.viewBehaviorBean('Company Registration Date','REQUIRED')}"
										requiredMessage="#{msg['lbl.validselectt']} #{msg['lbl.companyaregdate']}"
										maxdate="#{corpregisterBean.dateAdderForReg}"
										disabled="#{corpregisterBean.disableDate}"
										readonly="#{corpregisterBean.readOnly}">

										<p:message for="regdate" display="text" />
									</p:calendar>
									 </p:column>
									<p:column>
									   	
									<h:outputText
										value="#{msg['lbl.email']} #{corpregisterBean.viewBehaviorBean('Email','MANDATORY')}" />
									</p:column>
									<p:column>
									    <p:inputText id="email" value="#{corpregisterBean.email}"
										required="#{corpregisterBean.viewBehaviorBean('Email','REQUIRED')}"
										requiredMessage="#{msg['lbl.validationprefix']} #{msg['lbl.email']}"
										validatorMessage="#{msg['lbl.validlemailformat']}"
										readonly="#{corpregisterBean.readOnly}">
										<f:validateRegex
											pattern="^[_A-Za-z0-9-\+]+(\.[_A-Za-z0-9-]+)*@[A-Za-z0-9-]+(\.[A-Za-z0-9]+)*(\.[A-Za-z]{2,})$" />
										<p:message for="email" display="text" />
								    	</p:inputText>
									</p:column>
									</p:row>
									<p:row>
									<p:column>
									<h:outputText
										value="#{msg['lbl.location']} #{corpregisterBean.viewBehaviorBean('Location','MANDATORY')}" />
									</p:column>
								
									<p:column>
										<p:message for="location" display="text"></p:message>
									<p:selectOneMenu value="#{corpregisterBean.location}" id="location"
										required="#{corpregisterBean.viewBehaviorBean('Location','REQUIRED')}"
										style="width:175px;color:black" filter="true" requiredMessage="#{msg['lbl.validselectt']} #{msg['lbl.location']}"
										filterMatchMode="startsWith">
										<f:selectItem itemLabel="#{msg['lbl.select']}" itemValue="" />
										<f:selectItems value="#{corpregisterBean.countryListFromDB}"
											var="c" itemLabel="#{c.countryName}"
											itemValue="#{c.countryId}"
											readonly="#{corpregisterBean.viewBehaviorBean('Location','READ_ONLY')}"
											disabled="#{corpregisterBean.viewBehaviorBean('Location','ENABLE')}"/>
									</p:selectOneMenu>
									</p:column>
									<p:column>
									<h:outputText
										value="#{msg['lbl.amlstatus']} #{corpregisterBean.viewBehaviorBean('AML Status','MANDATORY')}" />
									</p:column>
									<p:column>
									<p:inputText id="amlStatus"
										value="#{corpregisterBean.AMLStatus}"
										disabled="true">
									</p:inputText>
									</p:column>
									</p:row>
									<p:row>
									<p:column>
									</p:column>
									<p:column>
									</p:column>
									<p:column>
									</p:column>
									<p:column>
									   <p:commandButton value="#{msg['lbl.validate']}"
										rendered="#{corpregisterBean.coValdtBtn}"
										actionListener="#{corpregisterBean.checkCompanyAMLStatus}"
										process="mainPanel" update="amlStatus,mainPanel1" 
										id="ajax" ajax="false"></p:commandButton>
									</p:column>
									
                                    </p:row>
                                    <p:row>
                                    <p:column>
									<p:ajaxStatus id="corporatestatuspanel">
										<f:facet name="start">
											<h:graphicImage value="../images/ajax_loader.gif" />
										</f:facet>
										<f:facet name="start">
											<h:graphicImage value="" />
										</f:facet>
									</p:ajaxStatus>
									</p:column>
									</p:row>
								</p:panelGrid>
							</p:panel>
							
							
							
							<p:panel id="mainPanel1"
								rendered="#{corpregisterBean.coAMLCommon}"
								style="align: left; width:820px;border:none;">
								
								<h:panelGrid columns="1"
										rendered="#{corpregisterBean.coAMLPass}" styleClass="heading2">
										<h:outputText value="#{msg['lbl.Objectives']}"
											rendered="#{corpregisterBean.coAMLPass}" />
									</h:panelGrid>
								<p:panel id="panel4" rendered="#{corpregisterBean.coAMLPass}"
									style="align: left; width:820px;  border:none; ">

									<p:panelGrid cellpadding="0" cellspacing="15"
										rendered="#{corpregisterBean.coAMLPass}"
										style="text-align:left;width: 91%;">
										<p:row>
											<p:column>
											</p:column>
											<p:column>
												<h:outputText
													value="#{msg['lbl.validselectt']} #{msg['lbl.primaryobjective']}"
													rendered="#{corpregisterBean.primayObjVisibility}"
													style="color:red;" />
											</p:column>
										</p:row>
										<p:row>
											<p:column style="width:192px;">
												<h:outputText
													value="#{msg['lbl.primaryobjective']} #{corpregisterBean.viewBehaviorBean('Primary Objective','MANDATORY')}"
													rendered="#{corpregisterBean.coAMLPass}" />
											</p:column>
											<p:column>
											    <p:message for="prmobj" display="text" />
												<p:selectOneMenu value="#{corpregisterBean.primaryObjId}"  id="prmobj"
													rendered="#{corpregisterBean.coAMLPass}"
													required="#{corpregisterBean.viewBehaviorBean('Primary Objective','REQUIRED')}"
													requiredMessage="#{msg['lbl.validselectt']} #{msg['lbl.primaryobjective']}" 
													style="width: 300px; height: 25px; margin-top:15px;text-align:left"
													readonly="#{corpregisterBean.viewBehaviorBean('Primary Objective','READ_ONLY')}"
													disabled="#{corpregisterBean.viewBehaviorBean('Primary Objective','ENABLE')}">
													<f:selectItem itemLabel="#{msg['lbl.select']}" itemValue="" />
													<f:selectItems value="#{corpregisterBean.objectivesFromDB}"
														var="o" itemLabel="#{o.objectiveDesc}"
														itemValue="#{o.objectiveId}" />
														
												</p:selectOneMenu>
											</p:column>
										</p:row>


									</p:panelGrid>

									<p:panelGrid cellpadding="0" cellspacing="15"
										rendered="#{corpregisterBean.coAMLPass}"
										style="text-align:left;width: 100%;">
										<p:row>
											<p:column>
											</p:column>
											<p:column>
												<h:outputText
													value="#{msg['lbl.validselectt']} #{msg['lbl.secondaryobjective']}"
													rendered="#{corpregisterBean.secondaryObjVisibility}"
													style="color:red;" />
											</p:column>
										</p:row>
										<p:row>
											<p:column style="width:192px;">
												<h:outputText
													value="#{msg['lbl.secondaryobjective']} #{corpregisterBean.viewBehaviorBean('Secondary Objective','MANDATORY')}"
													rendered="#{corpregisterBean.coAMLPass}" />
											</p:column>
											<p:column>
												<p:selectOneMenu value="#{corpregisterBean.seconObj}"
													rendered="#{corpregisterBean.coAMLPass}"
													 
													style="width:300px; height: 25px; margin-top:15px;text-align:left"
													readonly="#{corpregisterBean.viewBehaviorBean('Secondary Objective','READ_ONLY')}"
													disabled="#{corpregisterBean.viewBehaviorBean('Secondary Objective','ENABLE')}">
													<f:selectItem itemLabel="#{msg['lbl.select']}" itemValue="" />
													<f:selectItems
														value="#{corpregisterBean.objectivesFromExistingList}"
														var="o" itemLabel="#{o.objectiveDesc}"
														itemValue="#{o.objectiveId}" />
												</p:selectOneMenu>
											</p:column>
										</p:row>

										<p:row>
											<p:column>
												<p:commandButton value="#{msg['lbl.add']}"
													rendered="#{corpregisterBean.coAMLPass}"
													actionListener="#{corpregisterBean.addSecondaryObjective}"
													id="addTable4" update="panel4" process="panel4">
												</p:commandButton>
											</p:column>
											<p:column>
												<h:outputText
													value="#{msg['lbl.duplicatesecondaryobjective']}"
													rendered="#{corpregisterBean.secondaryObjDuplicate}"
													style="color:red;" />
													<h:outputText
													value="#{msg['lbl.atleastone']} #{msg['lbl.secondaryobjective']}"
													rendered="#{corpregisterBean.secondaryListVisibility}"
													style="color:red;" />
											</p:column>
											
										</p:row>



									</p:panelGrid>
									<br />
									<p:dataTable value="#{corpregisterBean.secondaryList}" var="s"
										editable="true"
										rendered="#{corpregisterBean.enableSecondaryObjDataTable}"
										id="datatable4" emptyMessage="#{msg['lbl.norecordfound']}">
										<!-- <p:ajax event="cellEdit"  listener="#{corpregisterBean. modifiedSecndryObj(s)}" /> -->
										<p:column>
											<f:facet name="header">
												<h:outputText value="#{msg['lbl.secondaryobjective']}" />
											</f:facet>
											<p:cellEditor>
												<f:facet name="output">
													<h:outputText value="#{s.seconObj}"
														style="text-align: left; font-size:13px;  height:20px; width: 98%;border:none !important;box-shadow:none !important; padding:0 !important;margin:0px !important;" />
												</f:facet>
												<f:facet name="input">
													<p:selectOneMenu value="#{s.secondaryObjectId}"
														rendered="#{corpregisterBean.coAMLPass}"
														style="width: 175px; ">
														<f:selectItem itemLabel="#{msg['lbl.select']}"
															itemValue="" />
														<f:selectItems value="#{s.lstSecoundryObjective}" var="o"
															itemLabel="#{o.objectiveDesc}"
															itemValue="#{o.objectiveId}" />
													</p:selectOneMenu>
												</f:facet>
											</p:cellEditor>
										</p:column>
										<p:column style="width:50px; text-align:center !important;">
											<f:facet name="header">
												<h:outputText value="#{msg['lbl.delete']}" />
											</f:facet>
											<p:commandButton styleClass="delete" value=""
												actionListener="#{corpregisterBean.removeSecondaryObjective(s)}"
												update="datatable4"></p:commandButton>
										</p:column>
									</p:dataTable>

								</p:panel>
								<p:panel id="panel" rendered="#{corpregisterBean.coAMLPass}"
									style="align:left;width:820px;border:none;">

									<h:panelGrid columns="1"
										rendered="#{corpregisterBean.coAMLPass}" styleClass="heading2">
										<h:outputText value="#{msg['lbl.contactdetails']}"
											rendered="#{corpregisterBean.coAMLPass}" />
									</h:panelGrid>


									<p:panelGrid cellpadding="0" cellspacing="15"
										rendered="#{corpregisterBean.coAMLPass}"
										style="text-align:left;width: 100%;">

										<p:row>
											<p:column>
											</p:column>
											<p:column>
												<h:outputText
													value="#{msg['lbl.validselectt']} #{msg['lbl.contacttype']}"
													style="color:red;"
													rendered="#{corpregisterBean.contactTypeVisibility}" />
											</p:column>
											<p:column>
											</p:column>
											<p:column>
												<h:outputText
													value="#{msg['lbl.validselectt']} #{msg['lbl.country']}"
													style="color:red;"
													rendered="#{corpregisterBean.countryVisibility}" />
											</p:column>
										</p:row>
										<p:row>
											<p:column>
												<h:outputText
													value="#{msg['lbl.contacttype']} #{corpregisterBean.viewBehaviorBean('Contact Type','MANDATORY')}"
													rendered="#{corpregisterBean.coAMLPass}" />
											</p:column>
											<p:column>
												<p:selectOneMenu id="newContactType"
													value="#{corpregisterBean.contType}"
													rendered="#{corpregisterBean.coAMLPass}"
													style="width: 175px; height: 25px; margin-top:15px;text-align:left"
													 
													readonly="#{corpregisterBean.viewBehaviorBean('Contact Type','READ_ONLY')}"
													disabled="#{corpregisterBean.viewBehaviorBean('Contact Type','ENABLE')}">
													<f:selectItem itemLabel="#{msg['lbl.select']}" itemValue="" />
													<f:selectItems
														value="#{corpregisterBean.contactTypeListDB}" var="o"
														itemLabel="#{o.contactType}"
														itemValue="#{o.contactTypeId}" />
												</p:selectOneMenu>
											</p:column>
											<p:column>
												<h:outputText
													value="#{msg['lbl.country']} #{corpregisterBean.viewBehaviorBean('Country','MANDATORY')}"
													rendered="#{corpregisterBean.coAMLPass}" />
											</p:column>
											<p:column>
												<p:selectOneMenu value="#{corpregisterBean.countryId}"
													rendered="#{corpregisterBean.coAMLPass}"
													 
													style="width:175px;" filter="true"
													filterMatchMode="startsWith"
													readonly="#{corpregisterBean.viewBehaviorBean('Country','READ_ONLY')}"
													disabled="#{corpregisterBean.viewBehaviorBean('Country','ENABLE')}">
													<f:selectItem itemLabel="#{msg['lbl.select']}" itemValue="" />
													<f:selectItems
														value="#{corpregisterBean.countryListFromDB}" var="c"
														itemLabel="#{c.countryName}" itemValue="#{c.countryId}" />
													<p:ajax update="state" event="change"
														listener="#{corpregisterBean.popState}" />
												</p:selectOneMenu>
											</p:column>
										</p:row>
										<p:row>
											<p:column>
											</p:column>
											<p:column>
												<h:outputText
													value="#{msg['lbl.validselectt']} #{msg['lbl.state']}"
													style="color:red;"
													rendered="#{corpregisterBean.stateVisibility}" />
											</p:column>
											<p:column>
											</p:column>
											<p:column>
												<h:outputText
													value="#{msg['lbl.validselectt']} #{msg['lbl.district']}"
													style="color:red;"
													rendered="#{corpregisterBean.districtVisibility}" />
											</p:column>
										</p:row>
										<p:row>
											<p:column>
												<h:outputText
													value="#{msg['lbl.state']} #{corpregisterBean.viewBehaviorBean('State','MANDATORY')}" />
											</p:column>
											<p:column>
												<p:selectOneMenu id="state"
													value="#{corpregisterBean.stateId}" style="width:175px;"
													 >
													<f:selectItem itemLabel="#{msg['lbl.select']}" itemValue="" />
													<f:selectItems value="#{corpregisterBean.stateFromDb}"
														var="c" itemLabel="#{c.stateName}"
														itemValue="#{c.stateId}"
														readonly="#{corpregisterBean.viewBehaviorBean('State','READ_ONLY')}"
														disabled="#{corpregisterBean.viewBehaviorBean('State','ENABLE')}"/>

													<p:ajax update="dist" event="change"
														listener="#{corpregisterBean.popDistrict}" />

												</p:selectOneMenu>
											</p:column>
											<p:column>
												<h:outputText
													value="#{msg['lbl.district']} #{corpregisterBean.viewBehaviorBean('Distict','MANDATORY')}   "
													style="text-align:left;" />
											</p:column>
											<p:column>
												<p:selectOneMenu value="#{corpregisterBean.distId}"
													style="width:175px;" id="dist"
													 >
													<f:selectItem itemLabel="#{msg['lbl.select']}" itemValue="" />
													<f:selectItems value="#{corpregisterBean.districFromDb}"
														var="c" itemLabel="#{c.districName}"
														itemValue="#{c.districId}"
														readonly="#{corpregisterBean.viewBehaviorBean('Distict','READ_ONLY')}"
														disabled="#{corpregisterBean.viewBehaviorBean('Distict','ENABLE')}"/>

													<p:ajax update="city" event="change"
														listener="#{corpregisterBean.popCity}" />

												</p:selectOneMenu>
											</p:column>
										</p:row>
										<p:row>
											<p:column>
											</p:column>
											<p:column>
												<h:outputText
													value="#{msg['lbl.validselectt']} #{msg['lbl.city']}"
													style="color:red;"
													rendered="#{corpregisterBean.cityVisibility}" />
											</p:column>
											<p:column>
											</p:column>
											<p:column>
												<h:outputText
													value="#{msg['lbl.validationprefix']} #{msg['lbl.localarea']}"
													style="color:red;"
													rendered="#{corpregisterBean.areaVisibility}" />
											</p:column>
										</p:row>
										<p:row>
											<p:column>
												<h:outputText
													value="#{msg['lbl.city']}  #{corpregisterBean.viewBehaviorBean('City','MANDATORY')} " />
											</p:column>
											<p:column>
												<p:selectOneMenu id="city"
													value="#{corpregisterBean.cityId}" style="width:175px;"
													rendered="#{corpregisterBean.coAMLPass}"
													 >
													<f:selectItem itemLabel="#{msg['lbl.select']}" itemValue=""
														readonly="#{corpregisterBean.viewBehaviorBean('City','READ_ONLY')}"
														disabled="#{corpregisterBean.viewBehaviorBean('City','ENABLE')}"/>
													<f:selectItems value="#{corpregisterBean.cityFromDb}"
														var="c" itemLabel="#{c.cityName}" itemValue="#{c.cityId}" />
												</p:selectOneMenu>
											</p:column>
											<p:column>
												<h:outputText
													value="#{msg['lbl.localarea']} #{corpregisterBean.viewBehaviorBean('Area','MANDATORY')}"
													rendered="#{corpregisterBean.coAMLPass}" />
											</p:column>
											<p:column>
												<p:inputText id="area" value="#{corpregisterBean.area}"
													requiredMessage="#{msg['lbl.validationprefix']} #{msg['lbl.localarea']}"
													rendered="#{corpregisterBean.coAMLPass}"
													 
													readonly="#{corpregisterBean.viewBehaviorBean('Area','READ_ONLY')}"
													maxlength="#{corpregisterBean.viewBehaviorBean('Area','MAX_VALUE')}"
													disabled="#{corpregisterBean.viewBehaviorBean('Area','ENABLE')}"
													validator="RegularExpressionValidation">
													<f:attribute name="regexValue"
														value="#{corpregisterBean.viewBehaviorBean('Area','PATTERN')}" />
													<f:attribute name="minValue"
														value="#{corpregisterBean.viewBehaviorBean('Area','MIN_VALUE')}" />
													<f:attribute name="maxValue"
														value="#{corpregisterBean.viewBehaviorBean('Area','MAX_VALUE')}" />
													<p:message for="area" display="text" />
												</p:inputText>
											</p:column>
										</p:row>
										<p:row>
											<p:column>
											</p:column>
