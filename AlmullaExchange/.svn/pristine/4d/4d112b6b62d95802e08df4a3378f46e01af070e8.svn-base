.ui-grid-a,.ui-grid-b,.ui-grid-c,.ui-grid-d,.ui-grid-solo {
	overflow: hidden;
}

.ui-outputlabel {
	font-size: 13px !important;
	padding-top: 10px;
	display: block;
}

.ui-block-a,.ui-block-b,.ui-block-c,.ui-block-d,.ui-block-e,.ui-block-f
	{
	border: 0 none;
	box-sizing: border-box;
	float: left;
	margin: 0;
	min-height: 1px;
	padding: 0;
}

.ui-block-a {
	clear: left;
}

[class*="ui-block-"]>button.ui-btn {
	margin-left: 0;
	margin-right: 0;
}

[class*="ui-block-"]>.ui-btn,[class*="ui-block-"]>.ui-select,[class*="ui-block-"]>.ui-checkbox,[class*="ui-block-"]>.ui-radio,[class*="ui-block-"]>button.ui-btn-inline,[class*="ui-block-"]>button.ui-btn-icon-notext
	{
	margin-left: 0.3125em;
	margin-right: 0.3125em;
}

.ui-grid-a>.ui-block-a {
	width: 18%;padding: 2.5% 1% 1% 2%;
}

.ui-grid-a>.ui-block-b {
	width: 24%;padding: 2% 1% 1% 2%;
}

.ui-grid-b>.ui-block-a,.ui-grid-b>.ui-block-b,.ui-grid-b>.ui-block-c {
	width: 33.333%;
}

.ui-grid-c>.ui-block-a,.ui-grid-c>.ui-block-b,.ui-grid-c>.ui-block-c,.ui-grid-c>.ui-block-d
	{
	width: 24.5%;
	padding: 0 0.5%;
}

.ui-grid-d>.ui-block-a,.ui-grid-d>.ui-block-b,.ui-grid-d>.ui-block-c,.ui-grid-d>.ui-block-d,.ui-grid-d>.ui-block-e
	{
	width: 19.5%;
	padding: 0 0.5%;
}

.ui-grid-e>.ui-block-a {
	width: 18%;
	padding: 2.7% 1% 1% 2%;
}

.ui-grid-e>.ui-block-b {
	width: 23%;
	padding: 2% 0% 1% 2%;
}

.ui-grid-e>.ui-block-c {
	width: 17%;
	padding: 2.7% 0% 1% 3%;
}

.ui-grid-e>.ui-block-d {
	width: 23%;
	padding: 2% 0% 1% 2%;
}

.ui-grid-e>.ui-block-e,.ui-grid-e>.ui-block-f {
	width: 3%;
	padding: 1%;
}

.ui-grid-solo>.ui-block-a {
	float: none;
	width: 100%;
}

.ui-input-text input,.ui-input-search input,textarea.ui-input-text {
	box-sizing: border-box;
	margin: 0 auto;
	display: block;
	line-height: 1.4em;
	outline: 0 none;
	padding: 0.4em;
	width: 90%;
}

.ui-inputfield,.ui-widget-content .ui-inputfield,.ui-widget-header .ui-inputfield
	{
	box-shadow: 0px 1px 4px #E4E4E4 inset !important;
	border: 1px solid #DBDBDB !important;
}

@media ( max-width : 768px) {
	.ui-responsive>.ui-block-a,.ui-responsive>.ui-block-b,.ui-responsive>.ui-block-c,.ui-responsive>.ui-block-d,.ui-responsive>.ui-block-e,.ui-responsive>.ui-block-f
		{
		float: none;
		width: 98%;
		margin: 0 auto;
	}
	
	.ui-grid-a>.ui-block-a {
	width:auto; padding: 0px 1% 0% 1% ;
}

.ui-grid-a>.ui-block-b {
	width:auto; padding: 10px 1% 2% 1%;
}
	.ui-grid-e>.ui-block-a {
		width: auto;
		padding: 10px 1% 0% 1%;
	}
	.ui-grid-e>.ui-block-b {
		width: auto;
		padding: 1% 1% 15px 1%;
	}
	.ui-grid-e>.ui-block-c {
		width: auto;
		padding: 10px 1% 0% 1%;
	}
	.ui-grid-e>.ui-block-d {
		width: auto;
		padding: 1% 1% 15px 1%;
	}
	.ui-grid-e>.ui-block-e,.ui-grid-e>.ui-block-f {
		display: none !important;
		visibility: hidden !important;
	}
	.ui-input-text input,.ui-input-search input,textarea.ui-input-text {
		width: 100%;
	}
	.ui-input-text input,.ui-input-search input {
		border: 0 none;
		margin: 0;
		text-align: left;
	}
}

@media ( max-width : 320px) {
	.ui-responsive>.ui-block-a,.ui-responsive>.ui-block-b,.ui-responsive>.ui-block-c,.ui-responsive>.ui-block-d,.ui-responsive>.ui-block-e,.ui-responsive>.ui-block-f
		{
		float: none;
		width: 98%;
		margin: 0 auto;
	}
	.ui-grid-e>.ui-block-e,.ui-grid-e>.ui-block-f {
		display: none !important;
		visibility: hidden !important;
	}
}