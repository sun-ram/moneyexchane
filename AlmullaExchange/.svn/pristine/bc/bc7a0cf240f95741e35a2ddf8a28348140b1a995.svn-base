<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <navigation-rule>
  <display-name>businesscomponentconf.xhtml</display-name>
  <from-view-id>ruleengine/businesscomponentconf.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>ruleengine/businesscomponentconf.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>businesscomponent.xhtml</display-name>
  <from-view-id>ruleengine/businesscomponent.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>ruleengine/businesscomponent.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>forgotpassword.xhtml</display-name>
  <from-view-id>/login/forgotpassword.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>/common/success.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>remmiterInfo.xhtml</display-name>
  <from-view-id>/registration/personaldetails.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>/common/success.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>remmiterreg.xhtml</display-name>
  <from-view-id>/registration/personaldetails.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>cancel</from-outcome>
   <to-view-id>/login/login.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>customerinfoinbranch.xhtml</display-name>
  <from-view-id>/registration/customerinfoinbranch.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>/common/successcustinfobranch.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>bankapplicability.xhtml</display-name>
  <from-view-id>/treasury/bankapplicability.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>/common/success.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <!--   <display-name>index.xhtml</display-name>
  <from-view-id>/index.xhtml</from-view-id> -->
  <navigation-case>
   <from-outcome>customerinfoinbranch.xhtml</from-outcome>
   <to-view-id>/registration/customerinfoinbranch.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>login.xhtml</display-name>
  <from-view-id>/login/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>remmiterInfo</from-outcome>
   <to-view-id>/registration/personaldetails.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>login.xhtml</display-name>
  <from-view-id>/login/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>branchHome</from-outcome>
   <to-view-id>/registration/branchhome.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>remmiterInfo</from-outcome>
   <to-view-id>/registration/remmiterreg.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>login.xhtml</display-name>
  <from-view-id>/login/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>/login/usersignup.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>customerRegManualBranch.xhtml</display-name>
  <from-view-id>/registration/custmanualbranchreg.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>next</from-outcome>
   <to-view-id>/registration/customerinfoinbranch.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>login</from-outcome>
   <to-view-id>/login/login.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>customerInfoInBranch.xhtml</display-name>
  <from-view-id>/registration/customerinfoinbranch.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>login</from-outcome>
   <to-view-id>/login/login.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>back</from-outcome>
   <to-view-id>/registration/custmanualbranchreg.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>cancel</from-outcome>
   <to-view-id>/login/login.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>pending</from-outcome>
   <to-view-id>/common/pendinginfo.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>header.xhtml</display-name>
  <from-view-id>/common/header.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>login</from-outcome>
   <to-view-id>/login/login.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <!-- Haheem Code -->
 <navigation-rule>
  <display-name>corporateregistration.xhtml</display-name>
  <from-view-id>/registration/corporateregistration.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>/registration/corporateregistrationadditionalinfo.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>cancel</from-outcome>
   <to-view-id>/registration/branchhome.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/login/usersignup.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>/common/usersuccess.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>cancel</from-outcome>
   <to-view-id>/login/login.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/search/searchcustomer.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>branchPage</from-outcome>
   <to-view-id>/registration/customerregistrationbranch.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <navigation-case>
   <from-outcome>login</from-outcome>
   <to-view-id>/login/login.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/registration/corporateregistrationadditionalinfo.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>/common/successcustinfobranch.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/common/usersuccess.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>login</from-outcome>
   <to-view-id>/login/login.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/registration/customerinformationverification.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>nominee</from-outcome>
   <to-view-id>/login/nomineeregistration.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/registration/customerregistrationbranch.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>nominee</from-outcome>
   <to-view-id>/login/nomineeregistration.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <validator>
  <validator-id>RegularExpressionValidation</validator-id>
  <validator-class>com.amg.exchange.util.RegularExpressionValidation</validator-class>
 </validator>
 <application>
  <view-handler>com.amg.exchange.bundle.MyViewHandler</view-handler>
  <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
  <locale-config>
   <default-locale>en</default-locale>
   <supported-locale>ar</supported-locale>
  </locale-config>
  <resource-bundle>
   <base-name>com.amg.exchange.messages.amgamx</base-name>
   <var>msg</var>
  </resource-bundle>
 </application>
